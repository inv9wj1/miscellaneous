onst handleOnCreateButtonClick = () => {
    // e.preventDefault();
    console.log("filename is ", createFileName);
    console.log("model name is ", modelFileName);

    console.log(JSON.stringify(inputValues));

    console.log("tab name is ", tabName);
    handleOnLoading(true); //THIS IS NOT TRANSFERRING THE TRUE TO THE FUNCTION
    console.log("loading is ", modalVisibleLoading);
    if (tabName === "model") {
      const token = Cookies.get("token"); // Get the token from the cookie
      axiosConnect
        .post(
          "/createSingleFileUsingApi",
          {
            createFileName: createFileName,
            modelFileName: modelFileName,
            inputValues: inputValues,
            tabName: tabName,
          },
          {
            headers: {
              "Access-Control-Allow-Origin": "*", // Replace * with your allowed origins
              "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
              "Access-Control-Allow-Headers":
                "Origin, X-Requested-With, Content-Type, Accept, Authorization",
              Authorization: `Bearer ${token}`, // Add the token here
            },
          }
        )

        .then((response) => {
          //handle success
          console.log(response);
          console.log("ganesh is here");
          setResponse(response);
          handleOnAnswer(true);
          setVisibleCreate(true);
          handleOnLoading(false);
          console.log("loading is ", modalVisibleLoading);
          loadFileSpaceData();
        })
        .catch((err) => {
          //handle error
          console.log(err);
          handleOnLoading(false);
          setVisibleCreate(true);
          console.log("loading is ", modalVisibleLoading);
          setResponse(err);
          handleOnAnswer(true);
        });
    }
